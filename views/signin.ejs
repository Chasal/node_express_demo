<%- include('./header') %>
  <div class="container">
    <%- include('./navigation') %>
    <%- include('./notification') %>
    <form class="form-signin form-horizontal" enctype="multipart/form-data" method="post">
      <div class="form-group">
        <label for="inputName" class="col-sm-2 control-label">用户名</label>
        <div class="col-sm-10">
          <input type="text" name="name" class="form-control" id="inputName" placeholder="用户名" required autofocus>
        </div>
      </div>
      <div class="form-group">
        <label for="inputPassword" class="col-sm-2 control-label">密码</label>
        <div class="col-sm-10">
          <input type="password" name="password" class="form-control" id="inputPassword" placeholder="密码" required autofocus>
        </div>
      </div>
      <div class="form-group">
        <div class="col-sm-offset-2 col-sm-10">
          <button class="btn btn-lg btn-primary btn-block" type="submit">登陆</button>
        </div>
      </div>
    </form>
  </div>
<%- include('./footer') %>
